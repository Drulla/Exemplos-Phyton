#declaração de um set {}
meu_set = {1,2,3,4,}
print(meu_set)

meu_set_2 = set([5,6,7,8])
print(meu_set_2)

#add - adiciona no set
meu_set.add(5)
print(meu_set)

#update() -atualizar elementos
meu_set.update([6,7,8])
print(meu_set)

#discard() - remove elemento de um set
meu_set.discard(2)
print(meu_set)

# Uniao de elementos
print(meu_set | meu_set_2) #ou
print(meu_set.union(meu_set_2))

#interseção - todos os elementos que existem em cada elemeentos
print(meu_set & meu_set_2) #ou
print(meu_set.intersection(meu_set_2))

#diferença entre dois sets
print(meu_set - meu_set_2) #ou
print(meu_set.difference(meu_set_2))

#Diferença simetrica
print(meu_set ^ meu_set_2) #ou
print(meu_set.symmetric_difference(meu_set_2))
